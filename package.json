{
  "name": "butlerui-manager-dashboard",
  "version": "0.0.1",
  "description": "Manager Dashboard for Butler System",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "json": "echo '=>JSON' && jsonparse src/assets/js/serverMessages/server_messages.json -o dist/assets/js/localization/",
    "browserify": "echo '=>Browserify' && browserify src/index.js -o ./dist/bundle.js -t [ babelify --presets [ es2015 react ] ]",
    "build-sass": "echo '=>Build SASS' && node-sass --output-style expanded  src/assets/css/main.scss dist/bundle.css  ",
    "copy": "echo '=>Copying...' && copy ./src/index.html ./dist/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://himanshu-saraswat@bitbucket.org/arsh_greyOrange/gor.butler.ui.managerdashboard.git"
  },
  "keywords": [
    "Manager",
    "Dashboard",
    "MD",
    "Butler",
    "UI"
  ],
  "author": "GOR Butler UI Team",
  "license": "ISC",
  "homepage": "https://bitbucket.org/arsh_greyOrange/gor.butler.ui.managerdashboard#readme",
  "devDependencies": {
    "babel-preset-es2015": "^6.13.2",
    "babel-preset-react": "^6.11.1",
    "babelify": "^7.3.0",
    "eslint": "^3.3.1",
    "node-sass": "^3.8.0"
  }
}
