{
  "name": "md-react-scripts",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "babel-plugin-react-intl": "^2.4.0",
    "moment-timezone": "^0.5.17",
    "react": "15.5.4",
    "react-dom": "15.5.4"
  },
  "devDependencies": {
    "babel-cli": "6.24.1",
    "babel-preset-es2015": "6.24.1",
    "d3": "3.5.6",
    "d3-time-format": "2.0.2",
    "d3-tip": "0.7.1",
    "enzyme": "2.9.1",
    "es6-symbol": "3.1.0",
    "fixed-data-table": "0.6.3",
    "node-sass": "4.5.2",
    "npm-run-all": "4.0.2",
    "postcss": "5.2.17",
    "postcss-cli": "3.2.0",
    "react": "15.3.1",
    "react-addons-create-fragment": "15.3.2",
    "react-addons-test-utils": "15.2.0",
    "react-d3-library": "1.1.7",
    "react-dimensions": "1.3.0",
    "react-dom": "15.2.0",
    "react-dropdown": "1.2.0",
    "react-hot-loader": "3.0.0-beta.5",
    "react-intl": "git+https://github.com/himanshuara/react-intl.git",
    "react-intl-cra": "0.2.0",
    "react-intl-redux": "0.1.0",
    "react-intl-webpack-plugin": "0.0.3",
    "react-paginate": "2.2.3",
    "react-redux": "4.4.5",
    "react-redux-modal": "0.4.3",
    "react-router": "2.7.0",
    "react-router-redux": "4.0.5",
    "react-scripts": "0.9.5",
    "react-test-renderer": "15.6.1",
    "redux": "3.5.2",
    "redux-mock-store": "1.2.2",
    "redux-router": "2.1.2",
    "sockjs-client": "1.1.4",
    "webstomp-client": "1.0.6"
  },
  "scripts": {
    "build-css": "node-sass src/assets/css/main.scss -o public/assets/ --output-style expanded ",
    "post-css": "postcss  --no-map --use autoprefixer -b 'last 2 versions' < public/assets/main.css | postcss  --no-map --use cssnano  > public/assets/main.min.css",
    "watch-css": "npm run build-css && node-sass src/assets/css/main.scss -o public/assets/ --watch --recursive --output-style expanded",
    "start-js": "react-scripts start",
    "build-dev": " npm-run-all -p watch-css start-js",
    "build-prod": "npm run build-css && npm run post-css && react-scripts build",
    "start": "npm run translate && REACT_APP_FILE_EXT= npm run build-dev",
    "build": "REACT_APP_FILE_EXT=.min npm run build-prod",
    "test": "react-scripts test --env=jsdom --coverage",
    "eject": "react-scripts eject",
    "translate": "react-intl-cra './src/**/*.js' './src/translations/en-US.json'"
  },
  "homepage": "./"
}
