{
  "name": "butlerui-manager-dashboard",
  "version": "0.0.1",
  "description": "Manager Dashboard for Butler System",
  "main": "index.js",
  "scripts": {
    "test": "./node_modules/jest/bin/jest.js",
    "webpack": "echo '=>Web-Pack' && webpack  --optimize-dedupe --display-error-details",
    "build-sass": "echo '=>Build SASS' && node-sass  --output-style expanded $npm_package_config_srcCss  $npm_package_config_destinationCss  ",
    "watch-sass": "node-sass --output-style expanded  $npm_package_config_watchCssSrc $npm_package_config_watchCssDist -v",
    "lint": "eslint src/**/*.js --cache --ignore-pattern .gitignore",
    "build-js": "npm run webpack",
    "watch:css": "onchange 'src/assets/css/**/*.scss' -- npm run build-sass ",
    "build": " npm run build-js && npm run build-sass",
    "watch:js": "npm run build && onchange 'src/**/*.js' -- npm run build-js",
    "watch:all": "parallelshell 'npm run watch:css' 'npm run watch:js'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://himanshu-saraswat@bitbucket.org/gorcode/gor.butler.ui.managerdashboard.git"
  },
  "config": {
    "destinationCss": "dist/assets/bundle.css",
    "srcCss": "src/assets/css/main.scss",
    "destinationJs": "./dist/assets/bundle.js",
    "srcJs": "src/index.js",
    "watchCssSrc": "src/assets/css/*.scss",
    "watchCssDist": "dist/assets/bundle.css"
  },
  "keywords": [
    "Manager",
    "Dashboard",
    "MD",
    "Butler",
    "UI"
  ],
  "author": "GOR Butler UI Team",
  "license": "ISC",
  "homepage": "https://bitbucket.org/arsh_greyOrange/gor.butler.ui.managerdashboard#readme",
  "devDependencies": {
    "argv": "0.0.2",
    "babel": "^6.5.2",
    "babel-cli": "^6.14.0",
    "babel-core": "^6.14.0",
    "babel-jest": "^13.2.2",
    "babel-loader": "^6.2.5",
    "babel-plugin-react-intl": "^2.2.0",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-preset-airbnb": "^2.0.0",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-2": "^6.13.0",
    "babelify": "^7.3.0",
    "copy-webpack-plugin": "^3.0.1",
    "d3": "^3.5.6",
    "d3-tip": "^0.7.1",
    "enzyme": "^2.4.1",
    "es6-symbol": "^3.1.0",
    "eslint": "^3.5.0",
    "eslint-config-standard": "^6.0.1",
    "eslint-plugin-promise": "^2.0.1",
    "eslint-plugin-react": "^6.1.2",
    "eslint-plugin-standard": "^2.0.0",
    "fs": "0.0.1-security",
    "glob": "^7.0.6",
    "html-webpack-plugin": "^2.22.0",
    "jasmine-reporters": "^1.0.0",
    "jest": "^13.2.3",
    "json-loader": "^0.5.4",
    "mkdirp": "^0.5.1",
    "node-sass": "^3.10.0",
    "npm": "^3.10.6",
    "onchange": "^2.5.0",
    "parallelshell": "^2.0.0",
    "react": "^15.3.1",
    "react-addons-test-utils": "^15.2.0",
    "react-d3-library": "^1.1.7",
    "react-dom": "^15.3.1",
    "react-intl-webpack-plugin": "0.0.3",
    "react-redux": "^4.4.5",
    "react-router": "^2.7.0",
    "react-router-redux": "^4.0.5",
    "react-simple-dropdown": "^1.1.4",
    "redux": "^3.5.2",
    "redux-router": "^2.1.2",
    "webpack": "^1.13.2",
    "webpack-cleanup-plugin": "^0.4.0",
    "webpack-dev-server": "^1.16.1"
  },
  "dependencies": {
    "d3-tip": "^0.7.1",
    "fixed-data-table": "^0.6.3",
    "istanbul": "^0.3.0",
    "jasmine": "^2.4.1",
    "react": "^15.3.1",
    "react-dimensions": "^1.3.0",
    "react-dom": "^15.2.0",
    "react-dropdown": "^1.1.0",
    "react-intl": "^2.1.5",
    "react-intl-redux": "^0.1.0",
    "react-redux": "^4.4.5",
    "react-redux-modal": "^0.4.3",
    "react-select": "^1.0.0-beta14",
    "xmldom": "^0.1.22"
  },
  "jest": {
    "unmockedModulePathPatterns": [
      "react",
      "react-redux",
      "<rootDir>/node_modules/enzyme/",
      "<rootDir>/node_modules/babel-runtime/"
    ]
  }
}
