{
  "name": "md-react-scripts",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "react": "15.5.4",
    "react-dom": "15.5.4",
    "sass-loader": "^7.0.3"
  },
  "devDependencies": {
    "autoprefixer": "6.7.2",
    "babel-cli": "6.24.1",
    "babel-core": "6.22.1",
    "babel-eslint": "7.1.1",
    "babel-jest": "18.0.0",
    "babel-loader": "6.2.10",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-react-app": "^2.2.0",
    "babel-runtime": "^6.20.0",
    "case-sensitive-paths-webpack-plugin": "1.1.4",
    "chalk": "1.1.3",
    "connect-history-api-fallback": "1.3.0",
    "cross-spawn": "4.0.2",
    "css-loader": "0.26.1",
    "d3": "3.5.6",
    "d3-time-format": "2.0.2",
    "d3-tip": "0.7.1",
    "detect-port": "1.1.0",
    "dotenv": "2.0.0",
    "enzyme": "2.9.1",
    "es6-symbol": "3.1.0",
    "eslint": "3.16.1",
    "eslint-config-react-app": "^0.6.2",
    "eslint-loader": "1.6.0",
    "eslint-plugin-flowtype": "2.21.0",
    "eslint-plugin-import": "2.0.1",
    "eslint-plugin-jsx-a11y": "4.0.0",
    "eslint-plugin-react": "6.4.1",
    "extract-text-webpack-plugin": "1.0.1",
    "file-loader": "0.10.0",
    "fixed-data-table": "0.6.3",
    "fs-extra": "0.30.0",
    "html-webpack-plugin": "2.24.0",
    "http-proxy-middleware": "0.17.3",
    "jest": "18.1.0",
    "json-loader": "0.5.4",
    "node-sass": "^4.5.2",
    "npm-run-all": "4.0.2",
    "object-assign": "4.1.1",
    "postcss": "5.2.17",
    "postcss-cli": "3.2.0",
    "postcss-loader": "1.2.2",
    "promise": "7.1.1",
    "react": "15.3.1",
    "react-addons-create-fragment": "15.3.2",
    "react-addons-test-utils": "15.2.0",
    "react-d3-library": "1.1.7",
    "react-dev-utils": "^0.5.2",
    "react-dimensions": "1.3.0",
    "react-dom": "15.2.0",
    "react-dropdown": "1.2.0",
    "react-hot-loader": "3.0.0-beta.5",
    "react-intl": "git+https://github.com/himanshuara/react-intl.git",
    "react-intl-cra": "0.2.0",
    "react-intl-redux": "0.1.0",
    "react-intl-webpack-plugin": "0.0.3",
    "react-paginate": "2.2.3",
    "react-redux": "4.4.5",
    "react-redux-modal": "0.4.3",
    "react-router": "2.7.0",
    "react-router-redux": "4.0.5",
    "react-test-renderer": "15.6.1",
    "redux": "3.5.2",
    "redux-mock-store": "1.2.2",
    "redux-router": "2.1.2",
    "sockjs-client": "1.1.4",
    "style-loader": "0.13.1",
    "url-loader": "0.5.7",
    "webpack": "1.14.0",
    "webpack-dev-server": "1.16.2",
    "webpack-manifest-plugin": "1.1.0",
    "webstomp-client": "1.0.6",
    "whatwg-fetch": "2.0.2"
  },
  "scripts": {
    "build-css": "node-sass src/assets/css/main.scss -o public/assets/ --output-style expanded ",
    "post-css": "postcss  --no-map --use autoprefixer -b 'last 2 versions' < public/assets/main.css | postcss  --no-map --use cssnano  > public/assets/main.min.css",
    "watch-css": "npm run build-css && node-sass src/assets/css/main.scss -o public/assets/ --watch --recursive --output-style expanded",
    "start-js": "node scripts/start.js",
    "build-dev": " npm-run-all -p watch-css start-js",
    "build-prod": "npm run build-css && npm run post-css && node scripts/build.js",
    "start": "npm run translate && REACT_APP_FILE_EXT= npm run build-dev",
    "build": "REACT_APP_FILE_EXT=.min npm run build-prod",
    "test": "node scripts/test.js --env=jsdom --coverage",
    "translate": "react-intl-cra './src/**/*.js' './src/translations/en-US.json'"
  },
  "homepage": "./",
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>[/\\\\](build|docs|node_modules|scripts)[/\\\\]"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    }
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  }
}
